{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "excludeDepNames": [
        "kubernetes",
        "openshift"
      ]
    },
    {
      "matchPackagePatterns": [
        "openshift",
        "xmltodict",
        "colorlog",
        "python-benedict",
        "packaging",
        "python-simple-logger",
        "kubernetes",
        "ipdb",
        "pytest"
      ],
      "groupName": "poetry-deps"
    }
  ]
}
